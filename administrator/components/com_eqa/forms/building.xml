<?xml version="1.0" encoding="utf-8" ?>
<form>
    <fieldset name="building">
        <field
                name="id"
                type="hidden"
            />

        <field
                name="code"
                type="text"
                label="COM_EQA_FORM_BUILDING_CODE_LABEL"
                required="true"
                pattern="[A-Z0-9]{2,10}"
        />
        <field
                name="description"
                type="textarea"
                label="COM_EQA_GENERAL_DESC"
        />
        <field
                name="created_by"
                type="text"
                readonly="true"
                label="COM_EQA_GENERAL_CREATED_BY"
        />
        <field
                name="created_at"
                type="text"
                readonly="true"
                label="COM_EQA_GENERAL_CREATED_AT"
        />
        <field
                name="updated_by"
                type="text"
                readonly="true"
                label="COM_EQA_GENERAL_UPDATED_BY"
        />
        <field
                name="updated_at"
                type="text"
                readonly="true"
                label="COM_EQA_GENERAL_UPDATED_AT"
        />
    </fieldset>
</form>