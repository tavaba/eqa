<?xml version="1.0" encoding="utf-8" ?>
<form>
    <fieldset name="addexamexaminees" addfieldprefix="Kma\Component\Eqa\Administrator\Field">
        <field
                name="classcode"
                type="text"
                required="true"
                label="COM_EQA_CLASS_CODE"
                description="COM_EQA_CLASS_CODE_DESC"
        />
        <field
                name="learnercodelist"
                type="textarea"
                required="true"
                label="COM_EQA_LEARNER_CODE"
        />
        <field
                name="attempt"
                type="examattempt"
                label="COM_EQA_EXAM_ATTEMPT"
                default=""
                required="true"
        >
            <option value="">COM_EQA_EXAM_ATTEMPT_REQUEST</option>
        </field>
        <field  name="ignore_error"
                type="radio"
                required="true"
                default="1"
                label="Bỏ qua các thí sinh đã có trong danh sách thi"
                class="btn-group btn-group-yesno"
        >
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
        <field  name="add_expired"
                type="radio"
                required="true"
                default="0"
                label="Thêm cả HVSV đã hết quyền thi"
                class="btn-group btn-group-yesno"
        >
            <option value="0">JNO</option>
            <option value="1">JYES</option>
        </field>
    </fieldset>
</form>